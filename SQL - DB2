Create Table

CREATE TABLE PA_PARAMETERIZATION_FIDELITY (
	ID INTEGER PRIMARY KEY NOT NULL GENERATED BY DEFAULT AS IDENTITY ( START WITH +1 , INCREMENT BY +1 , NO CACHE ),
	FIDELITY VARCHAR(20),
	STATUS CHAR(1) CHECK (STATUS IN ('S','N')),
    CREATED_BY VARCHAR(100),
	DT_CREATED TIMESTAMP DEFAULT CURRENT TIMESTAMP
);

